{
  "id": null,
  "uid": "yts-api-observability",
  "title": "YTS API Observability",
  "tags": ["yts", "api", "mlops"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Predict Request Count",
      "id": 1,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "http_requests_total{path=\"/predict\"}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Predict Latency Sum",
      "id": 2,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "http_request_latency_seconds_sum{path=\"/predict\"}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Paths by Requests",
      "id": 3,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "topk(10, http_requests_total)",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ]
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
}
